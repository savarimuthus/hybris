<types xmlns="http://www.hybris.com/schema/ondemand/domain"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.hybris.com/schema/ondemand/domain http://www.hybris.com/schema/ondemand/domain-1.6.xsd"
	package="com.hybris.datahub.trail.domain">
	<managedobjects>
		<managedobject code="CanonicalCustomer" extends="CanonicalItem">
			<description>The canonical view of a customer in the Data Hub</description>
		</managedobject>

		<managedobject code="CanonicalCustomerAddressRel"
			extends="CanonicalItem">
			<description>The canonical view of the many to many relationship between a Customer and Addresses in the Data Hub
   </description>
		</managedobject>

		<managedobject code="CanonicalAddress" extends="CanonicalItem">
			<description>The canonical view of a address in the Data Hub</description>
		</managedobject>
	</managedobjects>
</types>